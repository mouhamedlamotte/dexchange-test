enum ActionType {
  TRANSFER_CREATED
  TRANSFER_PROCESSING
  TRANSFER_SUCCESS
  TRANSFER_FAILED
  TRANSFER_CANCELED
}

model Action {
  id         String      @id @default(cuid())
  type       ActionType

  transactionId String
  transaction   Transaction @relation(fields: [transactionId], references: [id])

    createdAt  DateTime    @default(now())
  updatedAt  DateTime    @updatedAt
}
